{
  "name": "nh-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "tslint": "tslint -p . -c tslint.json",
    "build": "next build",
    "start": "next start src/client",
    "dev:client": "next src/client",
    "dev:server": "babel src/server --out-dir dist/server --source-maps --watch",
    "devnew": "npm run dev:client & npm run dev:server",
    "build:client": "next build src/client",
    "build:server": "babel src/server --out-dir dist/server --source-maps",
    "buildnew": "npm run build:client && npm run build:server",
    "preserve": "npm run buildnew",
    "serve": "cross-env NODE_ENV=production firebase serve --only functions,hosting",
    "predeploy": "rimraf dist/ && npm run buildnew",
    "deploy": "cross-env NODE_ENV=production firebase deploy --only functions,hosting"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@svgr/webpack": "^5.4.0",
    "@types/js-cookie": "^2.2.6",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-context": "^1.0.20",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "firebase": "^7.14.2",
    "firebase-admin": "^8.12.1",
    "firebase-functions": "^3.6.1",
    "graphql": "^15.0.0",
    "js-cookie": "^2.2.1",
    "next": "9.4.1",
    "next-cookies": "^2.0.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "style-loader": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@types/classnames": "^2.2.10",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "main": "dist/server/index.js",
  "engines": {
    "node": "10"
  }
}
